{
  "redirects": [
    {
      "source": "/(.*)/fella.css",
      "destination": "/$1/fella.min.css"
    },
    {
      "source": "/",
      "destination": "https://floo.fi/"
    }
  ],
  "headers": [
    {
      "source": "(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=3600, maxage=86400, stale-while-revalidate=300, stale-if-error=86400"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
